<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Pr1RV" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="LiveAR">
				<Option output="bin/LiveAR" prefix_auto="1" extension_auto="1" />
